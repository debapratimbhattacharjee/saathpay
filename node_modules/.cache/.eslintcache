ation status\r\n  const navigate = useNavigate(); // Initialize useNavigate\r\n\r\n  const handleChange = (e) => {\r\n    setOtp(e.target.value);\r\n  };\r\n\r\n  const handleSubmit = (e) => {\r\n    e.preventDefault();\r\n\r\n    // Basic OTP validation logic\r\n    if (otp.length === 6) {\r\n      setIsVerified(true); // OTP is verified\r\n      navigate('/money-transfer'); // Navigate to Money Transfer page\r\n    } else {\r\n      alert('Please enter a valid 6-digit OTP');\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className=\"otp-section mt-sm-5\">\r\n      <div className=\"otp-container\">\r\n        <div className=\"logo-container\"></div>\r\n        <h2>Enter the OTP</h2>\r\n        <p>We sent a 6-digit OTP to your registered phone number.</p>\r\n\r\n        {/* OTP Form */}\r\n        <form onSubmit={handleSubmit}>\r\n          <input\r\n            type=\"text\"\r\n            className=\"form-control otp-input\"\r\n            placeholder=\"Enter OTP\"\r\n            value={otp}\r\n            onChange={handleChange}\r\n            maxLength=\"6\"  // Limit to 6 characters for OTP\r\n            required\r\n          />\r\n          <button type=\"submit\" className=\"btn btn-primary nav_btn\">\r\n            Verify OTP\r\n          </button>\r\n        </form>\r\n\r\n        {/* Verification Status */}\r\n        {isVerified && <p className=\"success-message\">OTP Verified Successfully!</p>}\r\n        <p>Didn't receive the OTP? <a href=\"#\">Resend OTP</a></p>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default OTPVerification;\r\n","C:\\Users\\debag\\OneDrive\\Desktop\\saathpe-master\\src\\components\\Home\\Home.jsx",["124","125"],"import React from 'react'\nimport Carousel from '../Carousel/Carousel'\nimport StockWork from '../StockWork/StockWork'\nimport Support from '../Support/Support'\nimport Timer from '../Timer/Timer'\nimport Faq from '../Faq/Faq'\n\nimport './Home.css'\nimport OurTeam from '../OurTeam/OurTeam'\n\nconst Home = 